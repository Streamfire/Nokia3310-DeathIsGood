script/source = "extends KinematicBody2D

export var speed:float = 0.75
export var dash_speed:float = 12.0
export var shot_cooldown:float = 0.2
export var dash_cooldown:float = 3

export var shot_scene:PackedScene= null
var direction = Vector2()
var lastAimed= Vector2(1,0)
var aiming= Vector2(0,0)

var dash:bool = false
var dashCD:float= 0.0
var shotCD:float=0.0


onready var animator = $PlayerAnimator

func _ready():
	animator.play(\"Walk_Right\")
	animator.stop(true)
	animator.seek(0.5,true)

func get_input():
	direction = Vector2(0,0)
	if Input.is_action_pressed('move_right'):
		direction.x += 1
	if Input.is_action_pressed('move_left'):
		direction.x -= 1
	if Input.is_action_pressed('move_down'):
		direction.y += 1
	if Input.is_action_pressed('move_up'):
		direction.y -= 1
	
	aiming=Vector2(0,0)
	if Input.is_action_pressed('aim_right'):
		aiming.x += 1
	if Input.is_action_pressed('aim_left'):
		aiming.x -= 1
	if Input.is_action_pressed('aim_down'):
		aiming.y += 1
	if Input.is_action_pressed('aim_up'):
		aiming.y -= 1
	
	
	if aiming != Vector2(0,0):
		lastAimed = aiming.normalized()
	elif direction != Vector2(0,0):
		lastAimed=direction
	
	if Input.is_action_just_pressed(\"dash\")and dashCD <= 0.0:
		dash=true
		dashCD=dash_cooldown
	
	if Input.is_action_pressed(\"shoot\") and shotCD <= 0.0:
		var shot = shot_scene.instance()
		shot.position=self.position
		shot.direction= lastAimed
		self.get_parent().add_child(shot)
		shotCD=shot_cooldown

func play_animation():
	var animationdir
	if(aiming != Vector2(0,0)):
		animationdir=aiming
	else:
		animationdir=direction
		
	match animationdir:
		Vector2(0,0):
			animator.stop(true)
			animator.seek(0.5,true)
		Vector2(1,0):
			animator.play(\"Walk_Right\")
		Vector2(0,1):
			animator.play(\"Walk_Down\")
		Vector2(0,-1):
			animator.play(\"Walk_Up\")
		Vector2(-1,0):
			animator.play(\"Walk_Left\")
		
		Vector2(1,1):
			animator.play(\"Walk_DownRight\")
		Vector2(1,-1):
			animator.play(\"Walk_UpRight\")
		Vector2(-1,1):
			animator.play(\"Walk_DownLeft\")
		Vector2(-1,-1):
			animator.play(\"Walk_UpLeft\")  
	
	if direction == Vector2(0,0):
		animator.stop(true)
		animator.seek(0.5,true)


func _physics_process(delta):
	get_input()
	if dash != true:
		move_and_collide(direction.normalized()*speed)
	else:
		move_and_collide(lastAimed.normalized()*dash_speed)
		dash=false	
	
	play_animation()
	#cds
	dashCD-=delta
	shotCD-=delta

"

