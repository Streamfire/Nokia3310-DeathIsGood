[sub_resource type="GDScript" id=1]
script/source = "extends KinematicBody2D

export var speed:float = 10
var direction = Vector2()

onready var animator = $PlayerAnimator

func _ready():
	animator.play(\"Walk_Right\")
	animator.stop(true)
	animator.seek(0.5,true)

func get_input():
	direction = Vector2(0,0)
	if Input.is_action_pressed('move_right'):
		direction.x += 1
	if Input.is_action_pressed('move_left'):
		direction.x -= 1
	if Input.is_action_pressed('move_down'):
		direction.y += 1
	if Input.is_action_pressed('move_up'):
		direction.y -= 1

func play_animation(dir:Vector2):
	match dir:
		Vector2(0,0):
			animator.stop(true)
			animator.seek(0.5,true)
		Vector2(1,0):
			animator.play(\"Walk_Right\")
		Vector2(0,1):
			animator.play(\"Walk_Down\")
		Vector2(0,-1):
			animator.play(\"Walk_Up\")
		Vector2(-1,0):
			animator.play(\"Walk_Left\")
		
		Vector2(1,1):
			animator.play(\"Walk_DownRight\")
		Vector2(1,-1):
			animator.play(\"Walk_UpRight\")
		Vector2(-1,1):
			animator.play(\"Walk_DownLeft\")
		Vector2(-1,-1):
			animator.play(\"Walk_UpLeft\")
	pass  

func _physics_process(delta):
	get_input()
	move_and_collide(direction.normalized()*speed)
	#self.position += (direction.normalized()*speed)
	play_animation(direction)

"

