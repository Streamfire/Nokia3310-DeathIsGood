script/source = "extends KinematicBody2D


export var HP:int = 1
export var speed:float = 0.75

var direction = Vector2()
var goaldirection = Vector2(0,0)

onready var animator = $Animator

func _ready():
	animator.play(\"Walk_Right\")
	animator.stop(true)
	animator.seek(0.5,true)

func determine_move():
	direction = Vector2(0,0)
	if goaldirection != Vector2(0,0):
		var angle = atan2( goaldirection.y, goaldirection.x );
		var octant =int(round( 8 * angle / (2*PI) + 8 )) % 8;
		match octant:
			0:
				direction= Vector2(1,0)
			1:
				direction= Vector2(1,-1)
			2:
				direction= Vector2(0,-1)
			3:
				direction= Vector2(-1,-1)
			4:
				direction= Vector2(-1,0)
			5:
				direction= Vector2(-1,1)
			6:
				direction= Vector2(0,1)
			7:
				direction= Vector2(-1,1)


func play_animation():
	var animationdir
	animationdir=direction
		
	match animationdir:
		Vector2(0,0):
			animator.stop(true)
			animator.seek(0.5,true)
		Vector2(1,0):
			animator.play(\"Walk_Right\")
		Vector2(0,1):
			animator.play(\"Walk_Down\")
		Vector2(0,-1):
			animator.play(\"Walk_Up\")
		Vector2(-1,0):
			animator.play(\"Walk_Left\")
		
		Vector2(1,1):
			animator.play(\"Walk_DownRight\")
		Vector2(1,-1):
			animator.play(\"Walk_UpRight\")
		Vector2(-1,1):
			animator.play(\"Walk_DownLeft\")
		Vector2(-1,-1):
			animator.play(\"Walk_UpLeft\")  
	
	if direction == Vector2(0,0):
		animator.stop(true)
		animator.seek(0.5,true)


func _physics_process(delta):
	if(HP>0):
		determine_move()
		move_and_collide(direction.normalized()*speed)
		
		play_animation()




func _on_Area2D_area_entered(area):
	
	if!(area.get_parent().direction + self.direction.normalized())==Vector2(0,0):
		HP-=1
		AudioBus.play_effect(\"res://Assets/Sounds/enemie life hit.wav\")
	if(HP<=0):
		animator.play(\"Die\")
	pass # Replace with function body.


func _on_Animator_animation_finished(anim_name):
	if(anim_name==\"Die\"):
		queue_free()
	pass # Replace with function body.


func _on_Timer_timeout():
	goaldirection=Global.Playernode.position-self.position
	goaldirection.y=goaldirection.y*-1
	pass # Replace with function body.
"

